[core]
    pager = bat -p
    editor = nvim
    excludesFile = ~/.gitignore
[push]
    default = simple
[merge]
    conflictstyle = diff3
    tool = vimdiff
[fetch]
    prune = true
[diff]
    tool = vimdiff
[url "git@gitlab.com:"]
    insteadOf = https://gitlab.com/
[url "git@bitbucket.org:"]
    insteadOf = https://bitbucket.org/
[filter "lfs"]
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
[commit]
    gpgsign = true
[pull]
    ff = only
    rebase = true
[oh-my-zsh]
	git-commit-alias = 7a3695aadf51aaa24e79175b1684d4836e31956d
